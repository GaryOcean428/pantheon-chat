{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "prebuild": "tsx scripts/export-constants-to-python.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:python": "cd qig-backend && python -m pytest tests/ -v",
    "test:all": "npm run test && npm run test:python",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "db:push": "drizzle-kit push",
    "migrate:json-to-db": "tsx scripts/migrate-json-to-db.ts",
    "test:integration": "tsx scripts/test-integration.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "playwright:install": "playwright install --with-deps",
    "docs:maintain": "python3 scripts/maintain-docs.py",
    "constants:export": "tsx scripts/export-constants-to-python.ts",
    "validate:geometry": "tsx scripts/validate-geometric-purity.ts",
    "validate:critical": "bash scripts/run-critical-enforcement.sh",
    "validate:critical:strict": "bash scripts/run-critical-enforcement.sh --strict",
    "validate:critical:verbose": "bash scripts/run-critical-enforcement.sh --verbose",
    "validate:critical:fallback": "bash scripts/run-critical-enforcement.sh --fallback",
    "validate:all": "npm run validate:critical && npm run lint && npm run check",
    "migrate:vocab": "cd qig-backend && python3 migrate_checkpoint_to_pg.py",
    "migrate:vocab:dry": "cd qig-backend && python3 migrate_checkpoint_to_pg.py --dry-run",
    "migrate:semantic": "cd qig-backend && source .venv/bin/activate && python3 migrate_to_semantic_embeddings.py",
    "migrate:semantic:dry": "cd qig-backend && source .venv/bin/activate && python3 migrate_to_semantic_embeddings.py --dry-run --verbose",
    "populate:vocab:real": "cd qig-backend && python3 populate_real_vocabulary.py",
    "populate:vocab:real:dry": "cd qig-backend && python3 populate_real_vocabulary.py --dry-run",
    "populate:vocab:clear": "cd qig-backend && source .venv/bin/activate && python3 populate_tokenizer_vocabulary.py --clear",
    "populate:english": "cd qig-backend && python3 populate_english_vocabulary.py",
    "populate:english:dry": "cd qig-backend && python3 populate_english_vocabulary.py --dry-run",
    "seed:vocab": "cd qig-backend && python3 seed_word_vocabulary.py",
    "seed:vocab:dry": "cd qig-backend && python3 seed_word_vocabulary.py --dry-run",
    "populate:vocab": "cd qig-backend && uv run python populate_tokenizer_vocabulary.py",
    "populate:vocab:dry": "cd qig-backend && uv run python populate_tokenizer_vocabulary.py --dry-run",
    "cleanup:bpe": "cd qig-backend && python3 scripts/cleanup_bpe_tokens.py",
    "cleanup:bpe:dry": "cd qig-backend && python3 scripts/cleanup_bpe_tokens.py --dry-run --show-samples",
    "init:vocab": "cd qig-backend && python3 initialize_tokenizer_vocabulary.py",
    "init:vocab:dry": "cd qig-backend && python3 initialize_tokenizer_vocabulary.py --dry-run",
    "agent:qig-purity": "codebuff --agent qig-purity-enforcer",
    "agent:iso-doc": "codebuff --agent iso-doc-validator",
    "agent:ethical": "codebuff --agent ethical-consciousness-guard",
    "agent:imports": "codebuff --agent import-canonicalizer",
    "agent:types": "codebuff --agent type-any-eliminator"
  },
